{% extends 'base_admin.html.twig' %}

{% block body %}
    <h1>Imagem</h1>

    <table>
        <tbody>
            <tr>
                <th>Idimagem</th>
                <td>{{ imagem.id }}</td>
            </tr>
            <tr>
                <th>Src</th>
                <td>{{ imagem.src }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_imagem_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_imagem_edit', { 'id': imagem.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
